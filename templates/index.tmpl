<body class="$currentPage">


<div id="dialog" style="display:none"></div>

  <div id="header" class="container_24">
  
    <div id="latest-message-bar" class="grid_24"></div>
    <div class="clear"></div>
    <div id="log-bar" class="grid_24"></div>
    <div class="clear"></div>
    <div id="scan-bar" class="grid_24"> </div>
    <div class="clear"></div>
  </div>
  
  <div id="content" class="container_24">
    <div id="" class="grid_5">
      <div id="list-pane" class="">
        <h2 id="devices"><a href="/">Devices</a></h2>
        <ul id="device-name-list">
            <li id="scanning-device-name-list">Scanning</li>
        </ul>
        <h2 id="logs" class="$getVar('logActive', '')"><a href="/logs">Logs</a></h2>
        <h2 id="skymote"><a href="/skymote">SkyMote</a></h2>
      </div>
    </div>
  
    <div class="grid_19">  
        $mainContent
    </div>
    <div class="clear"></div>
  </div>
  <div id="footer" class="container_24">
    <div id="" class="grid_24 "></div>
    <div class="clear"></div>
  </div>

            
